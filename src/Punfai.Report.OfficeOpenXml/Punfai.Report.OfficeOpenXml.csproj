<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net461</TargetFrameworks>
    <DebugType>portable</DebugType>
    <AssemblyName>Punfai.Report.OfficeOpenXml</AssemblyName>
    <PackageId>Punfai.Report.OfficeOpenXml</PackageId>
    <PackageVersion>0.1.2</PackageVersion>
    <Authors>Nik Petroff</Authors>
    <Description>docx xlsx pptx support for Punfai report scripting engine</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Punfai. All rights reserved.</Copyright>
    <PackageTags>reporting reports</PackageTags>
    <FileVersion>0.1.2.0</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Joelharkes.DocumentFormat.OpenXml" Version="2.7.0" />
    <PackageReference Include="Punfai.Report" Version="0.1.0" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>
</Project>